            if (a1[i] == '4') {
                cout << "сорок" << " ";
            }
            if (a1[i] == '3') {
                cout << "тридцать" << " ";
            }
            if (a1[i] == '2') {
                cout << "двадцать" << " ";
            }
            if (a1[i] == '1') {
                cout << "десять" << " ";
            }
        }
        if ((i==1) and (a1[1]=='1')) {
            if (a1[i-1] == '9') {
                cout << "девятнадцать" << " ";
            }
            if (a1[i-1] == '8') {
                cout << "восемнадцать" << " ";
