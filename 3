            if (a1[i-1] == '5') {
                cout << "пятнадцать" << " ";
            }
            if (a1[i-1] == '4') {
                cout << "четырнадцать" << " ";
            }
            if (a1[i-1] == '3') {
                cout << "тринадцать" << " ";
            }
            if (a1[i-1] == '2') {
                cout << "двенадцать" << " ";
            }
            if (a1[i-1] == '1') {
                cout << "одиннадцать" << " ";
            }
        }
        if ((i==0) and (a1[i+1]!='1')) {
            if (a1[i] == '9') {
                cout << "девять" << " ";
            }
            if (a1[i] == '8') {
                cout << "восемь" << " ";
            }
            if (a1[i] == '7') {
                cout << "семь" << " ";
            }
            if (a1[i] == '6') {
                cout << "шесть" << " ";
            }
            if (a1[i] == '5') {
                cout << "пять" << " ";
            }
            if (a1[i] == '4') {
                cout << "четыре" << " ";
            }
            if (a1[i] == '3') {
                cout << "три" << " ";
            }
            if (a1[i] == '2') {
                cout << "два" << " ";
            }
            if (a1[i] == '1') {
                cout << "один" << " ";
            }
        }





    }
    if ((a1[0]=='0') and (c==1)){
        cout<<"ноль";
    }
    if ((a1[0]=='0') and (c==2) and (a1[1]=='1')){
        cout<<"десять";
